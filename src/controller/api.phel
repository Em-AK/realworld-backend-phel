(ns realworld\controller\api
  (:require phel\http :as h)
  (:require phel\json :as json))

(defn list-tags [req]
  (let [tags '("clojure" "phel" "fennel")]
    (h/response-from-map
     {:status 200
      :headers {"Content-Type" "application/json; charset=utf-8"}
      :body (json/encode {:tags tags})})))
